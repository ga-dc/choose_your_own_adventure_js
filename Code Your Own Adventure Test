console.log("Youat a Justin Bieber concert, and you hear this lyric 'Lace my shoes off, start racing.'");
console.log("Suddenlyber stops and says, 'Who wants to race me?'");
var userAnswer = prompt("Do you want to race Bieber on stage?");
if (userAnswer == "yes")
{
    console.log("YouBieber start racing. It's neck and neck! You win by a shoelace!");
}
else
{
    console.log("OhBieber shakes his head and sings 'I set a pace, so I can race without pacing.'");
}
